{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "node"
    ]
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts",
    "node_modules/@angular"
  ],
  "angularCompilerOptions": {
    "enableIvy": false  // Disable Ivy
  }
}
